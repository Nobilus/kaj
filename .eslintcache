ht: number | undefined;\n  }>({\n    width: undefined,\n    height: undefined,\n  });\n\n  useEffect(() => {\n    // Handler to call on window resize\n    function handleResize() {\n      // Set window width/height to state\n      setWindowSize({\n        width: window.innerWidth,\n        height: window.innerHeight,\n      });\n    }\n\n    // Add event listener\n    window.addEventListener(\"resize\", handleResize);\n\n    // Call handler right away so state gets updated with initial window size\n    handleResize();\n\n    // Remove event listener on cleanup\n    return () => window.removeEventListener(\"resize\", handleResize);\n  }, []); // Empty array ensures that effect is only run on mount\n\n  return windowSize;\n}\n\nexport default useWindowSize;\n","/home/jonas/Documents/Work/Nobilus/kaj/src/Utils/endpoints.ts",["151"],"export default {\n  menuitems: \"wp-api-menus/v2/menus/25\",\n  homepageposts: \"wp/v2/posts?per_page=3&order=desc&order_by=date&_embed\",\n  categories: \"wp/v2/categories?exclude=1, 16\",\n  postsbycat: \"wp/v2/posts?filter[cat]=\",\n  postbyid: \"wp/v2/posts/\",\n  allposts: \"wp/v2/posts\",\n  pagebyslug: \"wp/v2/pages/\",\n  getproducts: \"wc/v3/products/\",\n};\n","/home/jonas/Documents/Work/Nobilus/kaj/src/Utils/fp/dateToReadableDate.ts",["152"],"/home/jonas/Documents/Work/Nobilus/kaj/src/Pages/BlogPost/index.tsx",["153"],"import React, { useEffect, useState } from \"react\";\nimport endpoints from \"Utils/endpoints\";\nimport dateToReadableDate from \"Utils/fp/dateToReadableDate\";\nimport { axiosI } from \"Utils/Types/axiosInstance\";\nimport { IBlogpost } from \"Utils/Types/blogpost\";\nimport parse from \"html-react-parser\";\nimport Breadcrumbs from \"Components/Breadcrumbs\";\n//@ts-ignore\nfunction BlogPost({ match }) {\n  const {\n    params: { postid },\n  } = match;\n\n  console.log(match);\n\n  const [data, setData] = useState<IBlogpost | undefined>(undefined);\n\n  useEffect(() => {\n    const fetchPost = async () => {\n 